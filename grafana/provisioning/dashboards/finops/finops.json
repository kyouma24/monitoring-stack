          "format": 1,
          "hide": false,
          "rawSQL": "SELECT\n  regexp_extract(\n    line_item_resource_id,\n    'natgateway/(nat-[a-z0-9]+)',\n    1\n  ) AS nat_id,\n  SUM(CAST(line_item_blended_cost AS double)) AS monthly_cost\nFROM \"athenacurcfn_finops\".\"finops\"\nWHERE\n  year = '2026'\n  AND product_product_name = 'Amazon Elastic Compute Cloud'\n  AND (\n        line_item_usage_type LIKE '%NatGateway-Hours%'\n        OR line_item_usage_type LIKE '%NatGateway-Bytes%'\n      )\nGROUP BY\n  regexp_extract(\n    line_item_resource_id,\n    'natgateway/(nat-[a-z0-9]+)',\n    1\n  )\nHAVING SUM(CAST(line_item_blended_cost AS double)) > 0\nORDER BY monthly_cost DESC;",
          "refId": "B"
        }
